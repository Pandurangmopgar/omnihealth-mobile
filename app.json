{
  "expo": {
    "name": "OmniHealth",
    "slug": "omnihealth-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.svg",
    "scheme": "omnihealth-mobile",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#0B1120",
    "splash": {
      "image": "./assets/images/splash.svg",
      "resizeMode": "contain",
      "backgroundColor": "#0B1120"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.omnihealth.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.svg",
        "backgroundColor": "#0B1120"
      },
      "package": "com.omnihealth.mobile"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/icon.svg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Allow OmniHealth to access your Face ID."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
